<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/content"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/test_count" />

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/target_count" />

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/target_persent" />
    </LinearLayout>

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/test_num"
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="18" />

        <TextView
            android:id="@+id/target_num"
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="5" />

        <TextView
            android:id="@+id/target_parent"
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="28%" />
    </LinearLayout>

    <View
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="@color/main_divider_line" />

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/eat_pre" />

        <TextView
            style="@style/effect_value"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/eat_after" />
    </LinearLayout>

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                style="@style/effect_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/gray_breakfast"
                android:text="@string/breakfast"
                android:textSize="@dimen/text_size_13" />
        </LinearLayout>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/breadfast_pre"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/breadfast_after"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>
    </LinearLayout>

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                style="@style/effect_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/gray_launch"
                android:text="@string/lunch" />
        </LinearLayout>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/lunch_pre"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/lunch_after"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>
    </LinearLayout>

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                style="@style/effect_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/gray_dinner"
                android:text="@string/dinner" />
        </LinearLayout>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/dinner_pre"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/dinner_after"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>
    </LinearLayout>

    <LinearLayout
        style="@style/effect_line"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                style="@style/effect_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/gray_sleep"
                android:text="@string/sleep_pre" />
        </LinearLayout>

        <com.dian.diabetes.widget.ValueWidget
            android:id="@+id/sleep_pre_pre"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </com.dian.diabetes.widget.ValueWidget>

        <View
            android:layout_width="0dip"
            android:layout_height="0dip"
            android:layout_weight="1" />
    </LinearLayout>

</LinearLayout>